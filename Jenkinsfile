// # Declarative Jenkins Pipeline 
pipeline {
    agent any 
    stages {
        stage("Initialization") {
            steps {
                // 
                echo "Initializating ..."
            }
        }
        stage("Build"){
            steps{
                //
                echo "Building artifact"
            }
        }
        stage("Test"){
            steps{
                echo "Testing ..."
            }
        }
        stage("Deploy"){
            steps{
                echo "Deploying ..."
            }
        }
    }
}